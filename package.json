    "url": "https://github.com/pipedrive/slack-groups/issues"
  },
  "homepage": "https://github.com/pipedrive/slack-groups#readme",
  "devDependencies": {
    "eslint": "^8.14.0",
    "eslint-config-pipedrive": "^12.0.1",
    "jest": "^28.0.3"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@slack/web-api": "^6.7.1",
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
  }
